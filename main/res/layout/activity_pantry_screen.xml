<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:fitsSystemWindows="true">


    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/pantryscreenbg" />

    <Button
        android:id="@+id/closepantry"
        android:layout_width="157dp"
        android:layout_height="32dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="12dp"
        android:background="@layout/pantryselect"
        android:fontFamily="@font/futura"
        android:text="@string/closebutton"
        android:textColor="#FFFFFF"
        android:textSize="10sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/oils"
        android:layout_width="49dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/beverages"
        android:layout_marginStart="164dp"
        android:background="@drawable/pantryoilicon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/beverages"
        android:layout_width="46dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/closepantry"
        android:layout_marginTop="294dp"
        android:background="@drawable/pantrybeverageicon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />


    <Button
        android:id="@+id/snacks"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/oils"
        android:layout_marginEnd="71dp"
        android:background="@drawable/pantrysnackicon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/fruits"
        android:layout_width="67dp"
        android:layout_height="66dp"
        android:layout_alignBottom="@+id/vegies"
        android:layout_alignStart="@+id/oils"
        android:background="@drawable/pantryfruiticon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/vegies"
        android:layout_width="62dp"
        android:layout_height="58dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/closepantry"
        android:layout_marginTop="190dp"
        android:background="@drawable/pantryvegetableicon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/condiments"
        android:layout_width="61dp"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/fruits"
        android:layout_alignEnd="@+id/meat"
        android:background="@drawable/pantrycondimenticon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/dairy"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="35dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="53dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/pantrydairyicon"
        android:textSize="17sp" />

    <Button
        android:id="@+id/grains"
        android:layout_width="59dp"
        android:layout_height="59dp"
        android:layout_alignBottom="@+id/dairy"
        android:layout_centerHorizontal="true"
        android:background="@drawable/pantrywheaticon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />

    <Button
        android:id="@+id/meat"
        android:layout_width="55dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/dairy"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="63dp"
        android:background="@drawable/pantrysteakicon"
        android:fontFamily="@font/futura"
        android:textColor="#000000"
        android:textSize="17sp" />


</RelativeLayout>